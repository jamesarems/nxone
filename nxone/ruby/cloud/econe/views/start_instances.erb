<StartInstancesResponse xmlns="http://ec2.amazonaws.com/doc/<%= erb_version %>/">
   <requestId><%= @request_id %></requestId>
  <instancesSet>
    <% erb_vms.each { |vm_hash| %>
    <item>
      <%= render_instance_id(vm_hash[:vm]) %>
      <currentState>
        <%= render_state(vm_hash[:vm]) %>
      </currentState>
      <previousState>
        <%= vm_hash[:erb_previous_state] %>
      </previousState>
    </item>
    <% } %>
  </instancesSet>
</StartInstancesResponse>